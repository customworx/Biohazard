<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="SetupTextStyle">
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="Base.TextAppearance.AppCompat.Widget.ActionBar.Menu"
        parent="@android:style/TextAppearance.Small">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="AbstractComposeArea">
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:background">@android:color/background_dark</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:colorBackgroundCacheHint">@android:color/background_dark</item>
        <item name="android:animateLayoutChanges">true</item>
    </style>
    <style name="ComposeFromTextViewStyle">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ffd32f2f</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:background">@android:color/background_dark</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="RecipientEditTextViewBase" parent="@android:style/Widget.AutoCompleteTextView">
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
        <item name="android:background">@android:color/background_dark</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">32.0dip</item>
        <item name="android:popupBackground">@android:color/background_dark</item>
        <item name="android:inputType">textEmailAddress|textMultiLine</item>
    </style>
    <style name="ChipAutocompleteDividerStyle">
        <item name="android:background">#2bffffff</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.5dip</item>
        <item name="android:layout_marginLeft">72.0dip</item>
    </style>
    <style name="ConversationListFadeBase" parent="@android:style/Widget.Holo.ListView">
        <item name="android:background">@android:color/background_dark</item>
        <item name="android:colorBackgroundCacheHint">@android:color/background_dark</item>
    </style>
    <style name="UnifiedEmailTheme" parent="@android:style/Theme.Material">
        <item name="android:windowBackground">@android:color/background_dark</item>
        <item name="android:listViewWhiteStyle">@android:style/Widget.Material.ListView</item>
        <item name="android:colorBackgroundCacheHint">@android:color/background_dark</item>
        <item name="android:actionModeBackground">@android:color/background_dark</item>
        <item name="android:colorEdgeEffect">#ffd32f2f</item>
    </style>
    <style name="MessageButtonStyleBase" parent="@android:style/Widget.Holo.Button.Borderless" />
    <style name="ActionBarStyle" parent="@android:style/Widget.Holo.ActionBar.Solid">
        <item name="android:background">#ffd32f2f</item>
    </style>
</resources>
