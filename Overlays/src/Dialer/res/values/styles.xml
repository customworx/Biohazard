<resources>

    <style name="AlertDialogTheme" parent="@android:style/Theme.Material.Dialog"/>

    <style name="TextAppearanceMedium" parent="@android:style/TextAppearance.Medium">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>

    <style name="TextAppearanceSmall" parent="@android:style/TextAppearance.Small">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#fffafafa</item>
    </style>

    <style name="BackgroundOnlyTheme" parent="@android:style/Theme.Material">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowNoDisplay">true</item>
    </style>

    <style name="InCallPopupMenuStyle" parent="@android:style/Theme.Material"/>

    <style name="InCallActionBarStyle" parent="@android:style/Widget.Material.ActionBar">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:background">#ffd32f2f</item>
    </style>

    <style name="DialtactsTheme1" parent="@android:style/Theme.Material">
        <item name="android:colorForeground">#ff131313</item>
        <item name="android:colorBackground">#ff000000</item>
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textColorSecondary">#ffdedede</item>
        <item name="android:colorControlActivated">#ffd32f2f</item>
        <item name="android:colorPrimary">#ffd32f2f</item>
        <item name="android:colorPrimaryDark">#ffc62828</item>
    </style>

    <style name="CallLogSpinnerStyle">
        <item name="android:textAppearance">@android:style/TextAppearance.Small</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:singleLine">true</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="DialtactsActionBarOverflow" parent="@android:style/Widget.Material.ActionButton.Overflow">
        <item name="android:src">@drawable/ic_overflow_menu</item>
    </style>

    <style name="DialtactsActionBarStyle" parent="android:style/Widget.Material.ActionBar">
        <item name="android:background">@android:color/background_dark</item>
        <item name="android:textColor">#ffffffff</item>
        <!--<item name="android:titleTextStyle">@style/DialtactsActionBarTitleText</item>-->
        <item name="android:height">56dp</item>
        <item name="android:elevation">2dp</item>
        <item name="android:icon">@android:color/transparent</item>
        <item name="android:contentInsetStart">72dp</item>
    </style>

