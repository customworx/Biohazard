<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ff545454"/>
            <stroke android:width="1.0px" android:color="#ff999999"/>
            <padding android:bottom="10.0dip" android:left="2.0dip" android:right="2.0dip" android:top="10.0dip"/>
            <corners android:bottomLeftRadius="4.0dip" android:bottomRightRadius="4.0dip" android:topLeftRadius="4.0dip" android:topRightRadius="4.0dip"/>
            <gradient android:angle="90.0" android:centerColor="#ff333333" android:endColor="#ff545454" android:startColor="#ff545454" android:type="linear"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <solid android:color="#ff545454"/>
            <stroke android:width="1.0px" android:color="#ff999999"/>
            <padding android:bottom="10.0dip" android:left="2.0dip" android:right="2.0dip" android:top="10.0dip"/>
            <corners android:bottomLeftRadius="4.0dip" android:bottomRightRadius="4.0dip" android:topLeftRadius="4.0dip" android:topRightRadius="4.0dip"/>
            <gradient android:angle="90.0" android:centerColor="#ff333333" android:endColor="#ff545454" android:startColor="#ff545454" android:type="linear"/>
        </shape>
    </item>
</selector>