<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pierx.biohazard.overlays">

    <application
        android:hasCode="false" />
    <overlay
        android:priority="${priorityLabel}"
        android:targetPackage="${targetPackageLabel}" />

    <meta-data
        android:name="theme"
        android:value="${themeName}" />
    <meta-data
        android:name="author"
        android:value="${themeAuthor}" />
</manifest>
