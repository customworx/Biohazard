<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.ShareDialog" parent="@android:style/Theme.Holo.Dialog">
        <item name="android:colorPrimary">#ffd32f2f</item>
        <item name="android:colorPrimaryDark">#ffc62828</item>
        <item name="android:colorAccent">#ffd32f2f</item>
    </style>
    <style name="Theme.Hangout.IncomingRing"
        parent="@android:style/Theme.Holo.DialogWhenLarge.NoActionBar">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <style name="Theme.InCallDialpad" parent="@android:style/Theme.Holo.Dialog.NoActionBar" />
    <style name="Theme.EmeraldSea.Settings" parent="@android:style/Theme.Holo"/>
    <style name="SettingsActionBarStyle" parent="@android:style/Widget.Material.Light.ActionBar">
        <item name="android:icon">@android:color/transparent</item>
        <item name="android:background">#ffd32f2f</item>
        <item name="android:displayOptions">homeAsUp|showHome|showTitle</item>
    </style>
    <style name="TextAppearance.Medium" parent="@android:style/TextAppearance.Medium.Inverse" />
    <style name="TextAppearance.Small" parent="@android:style/TextAppearance.Small.Inverse" />
    <style name="Themeable.Hangouts" parent="@android:style/Theme.DeviceDefault.Dialog" />
    <style name="StatusMessageMenuButtonStyle">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:textSize">14.0sp</item>
        <item name="android:background">#ff000000</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:textAllCaps">true</item>
    </style>
    <style name="SettingsStyle" parent="@android:style/Theme.Material">
        <item name="android:actionBarStyle">@style/SettingsActionBarStyle</item>
        <item name="android:colorPrimary">#ffd32f2f</item>
        <item name="android:colorPrimaryDark">#ffc62828</item>
        <item name="android:colorAccent">#ffd32f2f</item>
        <item name="android:statusBarColor">#ffc62828</item>
    </style>
    <style name="AccountsSwitcher">
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textColorSecondary">#fffafafa</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="AccountsSwitcher.AccountDisplayNameStyle" parent="@style/AccountsSwitcher">
        <item name="android:textColorPrimary">#fffafafa</item>
        <item name="android:textColorSecondary">#fffafafa</item>
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">bold|italic</item>
        <item name="android:textColor">#fffafafa</item>
    </style>
    <style name="horizontalDividerStyle">
        <item name="android:background">#00000000</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.0dip</item>
    </style>
    <style name="verticalDividerStyle">
        <item name="android:background">#00000000</item>
        <item name="android:layout_width">1.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginTop">12.0dip</item>
        <item name="android:layout_marginBottom">12.0dip</item>
    </style>
    <style name="inviteVerticalDividerStyle">
        <item name="android:background">#00000000</item>
        <item name="android:layout_width">1.0dip</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="Platform.AppCompat" parent="@android:style/Theme.Material">
        <item name="android:statusBarColor">#ffc62828</item>
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textColorSecondary">#fffafafa</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:windowActionBar">false</item>
    </style>
    <style name="Platform.AppCompat.Light" parent="@android:style/Theme.Material">
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textColorSecondary">#fffafafa</item>
        <item name="android:windowBackground">@android:color/background_dark</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:windowActionBar">false</item>
    </style>
</resources>
