<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Platform.AppCompat.Light" parent="@android:style/Theme.Material">
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textColorSecondary">#ffffffff</item>
        <item name="android:textColorPrimaryInverse">#ffffffff</item>
        <item name="android:textColorSecondaryInverse">#b3ffffff</item>
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:textColorHighlight">#ffd32f2f</item>
        <item name="android:textColorHint">#b3ffffff</item>
        <item name="android:textColorLink">#ffd32f2f</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:colorPrimary">#ffd32f2f</item>
        <item name="android:colorPrimaryDark">#ffc62828</item>
        <item name="android:colorAccent">#ffd32f2f</item>
    </style>
    <style name="DialogActivityStyle" parent="@android:style/Theme.Material.Dialog">
        <item name="android:textColor">#ffbebebe</item>
    </style>
    <style name="common.Theme.Base.NoAppCompat.Light.Dialog"
        parent="@android:style/Theme.Material.Dialog">
        <item name="buttonBarStyle">?android:buttonBarStyle</item>
        <item name="buttonBarButtonStyle">?android:buttonBarButtonStyle</item>
        <item name="dividerHorizontal">?android:dividerHorizontal</item>
    </style>
    <style name="common.Theme.Base.NoAppCompat.Light.Dialog.MinWidth"
        parent="@android:style/Theme.Material.Dialog.MinWidth">
        <item name="buttonBarStyle">?android:buttonBarStyle</item>
        <item name="buttonBarButtonStyle">?android:buttonBarButtonStyle</item>
        <item name="dividerHorizontal">?android:dividerHorizontal</item>
    </style>
    <style name="drive_ProgressBarHorizontalIndeterminate"
        parent="@android:style/Widget.Material.ProgressBar.Horizontal">
        <item name="android:indeterminateOnly">true</item>
    </style>
    <style name="drive_Spinner" parent="@android:style/Widget.Material.Spinner.Underlined" />
    <style name="AlertThemeSelector" parent="@android:style/Theme.Material.Panel">
        <item name="android:backgroundDimAmount">0.6</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="PeopleButton.Borderless"
        parent="@android:style/Widget.Material.Button.Borderless" />
    <style name="PeopleDialogTheme" parent="@android:style/Theme.Material.Dialog" />
    <style name="Platform.AppCompat.Light.Dialog" parent="@android:style/Theme.Material.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
    </style>
    <style name="drive_Button_Platform" parent="@android:style/Widget.Material.Button.Borderless" />
    <style name="Base.ThemeOverlay.AppCompat.Light"
        parent="@android:style/ThemeOverlay.Material.Dark">
        <item name="android:colorControlNormal">#ffffffff</item>
        <item name="android:colorControlHighlight">#ffd32f2f</item>
        <item name="isLightTheme">false</item>
        <item name="colorControlHighlight">#8bd32f2f</item>
    </style>
    <style name="Base.TextAppearance.AppCompat.SearchResult.Title"
        parent="@android:style/TextAppearance.Material.SearchResult.Title" />
    <style name="SetBackupAccountActivity" parent="@android:style/Theme.Material.Dialog" />
    <style name="TrustAgentNotificationWithNoActionBar"
        parent="@android:style/Theme.Material.NoActionBar" />
</resources>
